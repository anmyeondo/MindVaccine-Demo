<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="40px">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="159dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:background="#FDFDFD"
            android:padding="40px"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="41dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="21dp"
                    android:layout_weight="1"
                    android:background="#FDFDFD"
                    android:padding="30px"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="41dp" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="53dp"
                    android:layout_height="23dp"
                    android:layout_weight="1"
                    android:baselineAligned="false"
                    app:srcCompat="@drawable/conslut_logo" />

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="4dp"
                    android:layout_weight="1"
                    android:background="#FDFDFD"
                    android:padding="30px"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="41dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView"
                android:layout_width="489dp"
                android:layout_height="match_parent"
                android:layout_marginTop="30px"
                android:background="#FFFFF"
                android:fontFamily="casual"
                android:padding="45px"
                android:text="추천 상담사"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="41dp" />
        </LinearLayout>

        <View
            android:id="@+id/divider"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="?android:attr/listDivider"
            android:backgroundTint="#989E9E"
            android:backgroundTintMode="src_over"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="94dp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="542dp"
        android:layout_height="40dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/search"
            android:layout_width="520dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="30px"
            android:background="@layout/consult_selectbutton"
            android:paddingLeft="5px">


            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                app:srcCompat="@drawable/selectbar" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:text=""
                android:textColor="@color/cardview_dark_background"
                android:textSize="10sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="370dp"
                android:layout_height="40dp"
                android:text="#스트레스 #불면증 #학업/진로 #직장/업무 #자존감"
                android:textAlignment="center"
                android:textColor="@color/cardview_dark_background"
                android:textSize="15sp"
                android:textStyle="bold" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/consultlayout1"
            android:layout_width="match_parent"
            android:layout_height="215dp"
            android:layout_marginStart="20px"
            android:layout_marginLeft="20px"
            android:layout_marginTop="20px"
            android:layout_marginEnd="20px"
            android:background="@layout/consult_frame"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/textView4"
                android:layout_width="343dp"
                android:layout_height="11dp" />

            <LinearLayout
                android:id="@+id/minilayout2"
                android:layout_width="88dp"
                android:layout_height="27dp"
                android:layout_marginLeft="25px"
                android:background="@layout/consult_mainbutton">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="9dp"
                    android:layout_height="14dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/crown" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="1.5dp"
                    android:layout_height="10dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="45dp"
                    android:layout_height="27dp"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-black"
                    android:text="베스트추천"
                    android:textColor="@color/white"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/consultlayout2"
            android:layout_width="match_parent"
            android:layout_height="16dp"
            android:orientation="vertical">

        </LinearLayout>

        <LinearLayout
            android:id="@+id/consultlayout3"
            android:layout_width="match_parent"
            android:layout_height="215dp"
            android:layout_marginStart="20px"
            android:layout_marginLeft="20px"
            android:layout_marginTop="20px"
            android:layout_marginEnd="20px"
            android:background="@layout/consult_frame"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/textView20"
                android:layout_width="343dp"
                android:layout_height="11dp" />

            <LinearLayout
                android:id="@+id/minilayout1"
                android:layout_width="67dp"
                android:layout_height="27dp"
                android:layout_marginLeft="25px"
                android:background="@layout/consult_mainbutton2">

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="7dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textColor="@color/cardview_dark_background"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/crown" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="10dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textColor="@color/cardview_dark_background"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:text="추천"
                    android:textColor="@color/cardview_dark_background"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>


        </LinearLayout>
    </LinearLayout>

</LinearLayout>