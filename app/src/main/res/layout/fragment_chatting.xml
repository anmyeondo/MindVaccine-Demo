<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="5dp">

        <TextView
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="65dp"
            android:textColor="#85CDB0"
            android:textSize="25dp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="675dp"
        android:background="#85CDB0"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/feed1"
            android:layout_width="320dp"
            android:layout_height="165dp"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:background="@layout/chatting_feed"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                app:srcCompat="@drawable/cat1" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="75dp"
                android:layout_height="20dp"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="85dp"
                android:text="떼껄룩 3호" />

            <TextView
                android:id="@+id/title1"
                android:layout_width="200dp"
                android:layout_height="30dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="8dp"
                android:text="과제가 하기 너무 싫어요"
                android:textSize="16dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/content1"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="40dp"
                android:text="할게 너무 많은데 과제가 자꾸 복사가 되요 어떻게 해야하죠?" />

            <View
                android:layout_width="320dp"
                android:layout_height="2dp"
                android:layout_marginTop="110dp"
                android:background="#5A6976" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="120dp"
                app:srcCompat="@drawable/like" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="120dp"
                android:text="힘내요 5" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="122dp"
                app:srcCompat="@drawable/sb2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginTop="120dp"
                android:text="댓글 8" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/feed2"
            android:layout_width="320dp"
            android:layout_height="162dp"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:background="@layout/chatting_feed"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                app:srcCompat="@drawable/cat2" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="85dp"
                android:text="떼껄룩 7호" />

            <TextView
                android:id="@+id/title2"
                android:layout_width="200dp"
                android:layout_height="30dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="8dp"
                android:text="요즘 삶이 힘드네"
                android:textSize="16dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/content2"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="40dp"
                android:text="오늘 하루 쯤은 놀아도 되겠지.... " />

            <View
                android:layout_width="320dp"
                android:layout_height="2dp"
                android:layout_marginTop="110dp"
                android:background="#5A6976" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="120dp"
                app:srcCompat="@drawable/like" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="120dp"
                android:text="힘내요 5" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="123dp"
                app:srcCompat="@drawable/sb2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginTop="120dp"
                android:text="댓글 3" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/feed3"
            android:layout_width="320dp"
            android:layout_height="163dp"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:background="@layout/chatting_feed"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                app:srcCompat="@drawable/cat3" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="85dp"
                android:text="떼껄룩 2호" />

            <TextView
                android:id="@+id/title3"
                android:layout_width="200dp"
                android:layout_height="30dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="8dp"
                android:text="코인을 시작했습니다."
                android:textSize="16dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/content3"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginLeft="93dp"
                android:layout_marginTop="40dp"
                android:text="일론 머스크가 제 돈을 뺏어 갔어요...." />

            <View
                android:layout_width="320dp"
                android:layout_height="2dp"
                android:layout_marginTop="110dp"
                android:background="#5A6976" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="120dp"
                app:srcCompat="@drawable/like" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="120dp"
                android:text="힘내요 7" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="122dp"
                app:srcCompat="@drawable/sb2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginTop="120dp"
                android:text="댓글 9" />
        </FrameLayout>
    </LinearLayout>
</LinearLayout>